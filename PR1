#include <iostream>

using namespace std;

struct P
{
	int x;
	int y;
	bool better;
};


void init(P *p, int* a, int N)
{
	int j=1;
	for(int i=0; i<N; i+=2)
	{
		p[j].x=a[i];
		p[j].y=a[i+1];
		p[j].better=0;
		j++;
	}
}
void dropB(P *p, int N)
{
	for(int i=0; i<N; ++i)
		p[i].better=0;
}
void CheckPlus(P* p, int N, P point)
{
	for(int i=0; i<N; ++i)
	{
		if(p[i].x>point.x&&p[i].y>point.y) p[i].better=1;
	}
}
void CheckMinus(P* p, int N, P point)
{
	for(int i=0; i<N; ++i)
	{
		if(p[i].x<point.x&&p[i].y<point.y) p[i].better=1;
	}
}

void ShowPlus(P* p, int N, P point)
{
	cout<<"POINT "<<point.x<<"."<<point.y<<endl;
	cout<<"P+:"<<endl;
	for(int i=0; i<N; ++i)
		if(p[i].better)
			cout<<p[i].x<<"."<<p[i].y<<endl;
}
void ShowMinus(P* p, int N, P point)
{
	cout<<"P-:"<<endl;
	for(int i=0; i<N; ++i)
		if(p[i].better)
			cout<<p[i].x<<"."<<p[i].y<<endl;
}
void ShowAll(P* p, int N)
{
	for(int i=0; i<N; ++i)
	{
		cout<<i+1<<" "<<p[i].x<<" "<<p[i].y<<" "<<p[i].better<<endl;
	}
}
int main()
{
	const int N=6;
	P p[N];
	int a[12]={2,5,3,3,1,4,1,3,4,3,5,4};
	init(p,a,12);

	for(int i=0; i<N; ++i)
	{
		CheckPlus(p,N,p[i]);
		ShowPlus(p,N,p[i]);
		dropB(p, N);
		CheckMinus(p,N,p[i]);
		ShowMinus(p,N,p[i]);
		dropB(p, N);
		cout<<endl;
	}
	return 0;
}
